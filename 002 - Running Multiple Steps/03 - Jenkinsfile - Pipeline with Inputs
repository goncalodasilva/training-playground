pipeline {
    agent { 
		docker { 
			image 'alpine:latest' 
		}
	}
	parameters {
		string(name: 'NAME', description: '')
		STRING(name: 'AGE', description: '') 
	}
    stages {
        stage('build') {
            steps {
                echo "$NAME"
                echo "$AGE"
            }
        }
    }
}
